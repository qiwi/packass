{
  "name": "@packasso/core",
  "dependencies": {
    "@antongolub/git-root": "1.5.10",
    "@semrel-extra/topo": "1.5.0",
    "@types/lodash-es": "4.17.6",
    "concurrently": "7.6.0",
    "cosmiconfig": "8.1.0",
    "fast-glob": "3.2.12",
    "lodash-es": "4.17.21",
    "read-pkg": "7.1.0"
  },
  "exports": {
    ".": {
      "development": "./src/main/ts/index.ts",
      "require": "./target/cjs/index.cjs",
      "import": "./target/esm/index.mjs",
      "types": "./target/esm/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/packasso.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/packasso/issues"
  },
  "homepage": "https://qiwi.github.io/packasso",
  "version": "0.0.0",
  "license": "MIT",
  "devDependencies": {
    "buildstamp": "2.1.1",
    "c8": "7.13.0",
    "earljs": "0.2.3",
    "eslint": "8.35.0",
    "eslint-config-qiwi": "2.0.9",
    "globby-cp": "1.1.3",
    "prettier": "2.8.4",
    "prettier-config-qiwi": "2.0.1",
    "rimraf": "4.2.0",
    "tsc-esm-fix": "2.20.12",
    "tsm": "2.3.0",
    "typescript": "4.9.5",
    "uvu": "0.5.6"
  },
  "type": "module",
  "main": "./target/cjs/index.cjs",
  "module": "./target/esm/index.mjs",
  "types": "./target/esm/index.d.ts",
  "files": [
    "target/cjs/**/*",
    "target/esm/**/*",
    "target/resources/**/*",
    "target/buildstamp.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "packasso": "tsm --conditions development ../cli/src/main/ts/index.ts"
  }
}
