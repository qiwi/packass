{
  "name": "@packasso/jest",
  "version": "1.0.0",
  "description": "jest",
  "keywords": [
    "jest",
    "preset"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/packasso.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/packasso/issues"
  },
  "homepage": "https://qiwi.github.io/packasso",
  "dependencies": {
    "@packasso/core": "workspace:*",
    "@packasso/jest-environment-jsdom": "workspace:*",
    "@packasso/jest-snapshot-resolver": "workspace:*",
    "@packasso/jest-svgr-transformer": "workspace:*",
    "@swc/core": "1.3.42",
    "@swc/jest": "0.2.24",
    "@types/jest": "29.5.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "rimraf": "4.4.1"
  },
  "exports": "./src/main/ts/index.ts",
  "license": "MIT",
  "publishConfig": {
    "type": "module",
    "main": "./target/cjs/index.cjs",
    "module": "./target/esm/index.mjs",
    "types": "./target/esm/index.d.ts",
    "exports": {
      ".": {
        "require": "./target/cjs/index.cjs",
        "import": "./target/esm/index.mjs",
        "types": "./target/esm/index.d.ts"
      }
    },
    "files": [
      "target/cjs/**/*",
      "target/esm/**/*",
      "target/buildstamp.json"
    ],
    "access": "public",
    "bin": {
      "packasso-jest": "./target/esm/bin.mjs"
    }
  },
  "scripts": {
    "packasso": "NODE_ENV=development NPM_CONFIG_YES=true npx tsx ../cli/src/main/ts/bin.ts"
  }
}
