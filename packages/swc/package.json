{
  "name": "@packasso/swc",
  "version": "0.0.0",
  "description": "swc",
  "keywords": [
    "swc",
    "build",
    "preset"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/packasso.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/packasso/issues"
  },
  "homepage": "https://qiwi.github.io/packasso",
  "dependencies": {
    "@packasso/core": "workspace:*",
    "@packasso/tsconfig": "workspace:*",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.42",
    "globby-cp": "1.1.3",
    "rimraf": "4.4.1",
    "tsc-esm-fix": "2.20.12",
    "typescript": "4.9.5"
  },
  "exports": {
    ".": {
      "development": "./src/main/ts/index.ts",
      "require": "./target/cjs/index.cjs",
      "import": "./target/esm/index.mjs",
      "types": "./target/esm/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "type": "module",
  "main": "./target/cjs/index.cjs",
  "module": "./target/esm/index.mjs",
  "types": "./target/esm/index.d.ts",
  "files": [
    "target/cjs/**/*",
    "target/esm/**/*",
    "target/buildstamp.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "packasso-swc": "./target/esm/bin.mjs"
  },
  "scripts": {
    "packasso": "npm_config_yes=true npx tsx --conditions development ../cli/src/main/ts/bin.ts"
  }
}
