{
  "name": "@packasso/core",
  "dependencies": {
    "@antongolub/git-root": "1.5.8",
    "@semrel-extra/topo": "1.4.4",
    "@types/lodash-es": "4.17.6",
    "concurrently": "7.6.0",
    "cosmiconfig": "8.0.0",
    "fast-glob": "3.2.12",
    "lodash-es": "4.17.21",
    "read-pkg": "7.1.0"
  },
  "exports": {
    ".": {
      "development": "./src/main/ts/index.ts",
      "require": "./target/cjs/index.cjs",
      "import": "./target/esm/index.mjs",
      "types": "./target/esm/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/packasso.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/packasso/issues"
  },
  "homepage": "https://qiwi.github.io/packasso",
  "license": "MIT",
  "type": "module",
  "main": "./target/cjs/index.cjs",
  "module": "./target/esm/index.mjs",
  "types": "./target/esm/index.d.ts",
  "files": [
    "target/cjs/**/*",
    "target/esm/**/*",
    "target/resources/**/*"
  ],
  "eslintConfig": {
    "extends": "eslint-config-qiwi"
  },
  "prettier": "prettier-config-qiwi",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "cmd": "yarn",
    "changelog": "changelog"
  }
}
