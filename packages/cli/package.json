{
  "name": "@packasso/cli",
  "version": "0.0.0",
  "description": "packasso cli",
  "keywords": [
    "cli"
  ],
  "bin": {
    "packasso": "./target/esm/bin.mjs",
    "packasso-cli": "./target/esm/bin.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/packasso.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/packasso/issues"
  },
  "homepage": "https://qiwi.github.io/packasso",
  "dependencies": {
    "@packasso/core": "workspace:*"
  },
  "exports": {
    ".": {
      "development": "./src/main/ts/index.ts",
      "require": "./target/cjs/index.cjs",
      "import": "./target/esm/index.mjs",
      "types": "./target/esm/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "type": "module",
  "main": "./target/cjs/index.cjs",
  "module": "./target/esm/index.mjs",
  "types": "./target/esm/index.d.ts",
  "files": [
    "target/cjs/**/*",
    "target/esm/**/*",
    "target/buildstamp.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "packasso": "npm_config_yes=true npx tsx --conditions development src/main/ts/bin.ts"
  }
}
