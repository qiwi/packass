{
  "name": "@packasso/cli",
  "dependencies": {
    "@packasso/core": "workspace:*",
    "@types/node": "18.15.3",
    "minimist": "1.2.8"
  },
  "bin": {
    "packasso": "./target/esm/index.mjs"
  },
  "exports": {
    ".": {
      "development": "./src/main/ts/index.ts",
      "require": "./target/cjs/index.cjs",
      "import": "./target/esm/index.mjs",
      "types": "./target/esm/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/packasso.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/packasso/issues"
  },
  "homepage": "https://qiwi.github.io/packasso",
  "version": "0.0.0",
  "scripts": {
    "packasso": "tsm --conditions development ../cli/src/main/ts/index.ts",
    "build": "yarn packasso build",
    "format": "yarn packasso format",
    "lint": "yarn packasso lint",
    "test": "yarn packasso test"
  },
  "license": "MIT",
  "devDependencies": {
    "buildstamp": "2.1.1",
    "c8": "7.13.0",
    "earljs": "0.2.3",
    "eslint": "8.35.0",
    "eslint-config-qiwi": "2.0.9",
    "globby-cp": "1.1.3",
    "prettier": "2.8.4",
    "prettier-config-qiwi": "2.0.1",
    "rimraf": "4.2.0",
    "tsc-esm-fix": "2.20.12",
    "tsm": "2.3.0",
    "typescript": "4.9.5",
    "uvu": "0.5.6"
  },
  "type": "module",
  "main": "./target/cjs/index.cjs",
  "module": "./target/esm/index.mjs",
  "types": "./target/esm/index.d.ts",
  "files": [
    "target/cjs/**/*",
    "target/esm/**/*",
    "target/resources/**/*",
    "target/buildstamp.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
